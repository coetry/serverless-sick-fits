{
  "version": 2,
  "name": "sick-fits",
  "builds": [
    {
      "src": "graphql.js",
      "use": "@now/node@canary",
      "config": { "awsLambdaHandler": "graphql.handler" }
    },
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/api/playground",
      "dest": "graphql.js"
    },
    {
      "src": "/api/graphql",
      "dest": "graphql.js"
    }
  ],
  "env": {
    "PRISMA_ENDPOINT": "@sick-fits-prisma-endpoint",
    "PRISMA_SECRET": "@sick-fits-prisma-secret"
  }
}
